{
	"name": "frontend",
	"version": "0.0.1",
	"scripts": {
		"dev": "cross-env NODE_OPTIONS=\"--experimental-vm-modules --no-warnings\" vite dev",
		"build": "cross-env CF_PAGES=1 vite build",
		"package": "cross-env CF_PAGES=1 svelte-kit package",
		"preview": "cross-env CF_PAGES=1 vite preview",
		"test": "playwright test",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. . && eslint .",
		"format": "prettier --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@esbuild-plugins/node-globals-polyfill": "^0.1.1",
		"@playwright/test": "^1.22.2",
		"@rgossiaux/svelte-heroicons": "^0.1.2",
		"@rollup/plugin-inject": "^4.0.4",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/kit": "next",
		"@tailwindcss/forms": "^0.5.2",
		"@tailwindcss/nesting": "^0.0.0-insiders.565cd3e",
		"@tailwindcss/typography": "^0.5.4",
		"@types/cookie": "^0.4.1",
		"@types/crypto-js": "^4.1.1",
		"@typescript-eslint/eslint-plugin": "^5.27.0",
		"@typescript-eslint/parser": "^5.27.0",
		"autoprefixer": "^10.4.7",
		"buffer": "^6.0.3",
		"cross-env": "^7.0.3",
		"cross-var": "^1.1.0",
		"daisyui": "^2.22.0",
		"eslint": "^8.16.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"is-ci": "^3.0.1",
		"miniflare": "^2.6.0",
		"postcss": "^8.4.14",
		"postcss-nesting": "^10.1.10",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"svelte": "^3.44.0",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^4.10.6",
		"svelte-typeahead": "^4.2.4",
		"tailwindcss": "^3.1.2",
		"tailwindcss-scrollbar": "^0.1.0",
		"theme-change": "^2.2.0",
		"tslib": "^2.3.1",
		"typescript": "^4.7.4",
		"util": "^0.12.4",
		"vite": "^3.0.0"
	},
	"type": "module",
	"dependencies": {
		"@auth0/auth0-spa-js": "^1.22.2",
		"@zxcvbn-ts/core": "^2.0.3",
		"@zxcvbn-ts/language-common": "^2.0.1",
		"@zxcvbn-ts/language-en": "^2.0.1",
		"crypto-js": "^4.1.1",
		"svelte-countup": "^0.2.6",
		"sveltekit-i18n": "^2.2.2"
	}
}
