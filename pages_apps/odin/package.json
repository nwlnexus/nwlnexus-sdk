{
  "name": "@nwlnexus/odin",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "vite:dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "test": "npm run test:integration && npm run test:unit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write .",
    "test:integration": "playwright test",
    "test:unit": "vitest",
    "dev": "wrangler pages dev --compatibility-date=${COMPAT_DATE} --compatibility-flag=nodejs_compat --proxy 5173 --port 8786 --inspector-port 9230 --persist-to=\"../../.wrangler\" -- pnpm run vite:dev",
    "pages:deploy": "pnpm run build && wrangler pages deploy .svelte-kit/cloudflare"
  },
  "devDependencies": {
    "@melt-ui/pp": "^0.3.0",
    "@melt-ui/svelte": "^0.70.0",
    "@playwright/test": "^1.41.0",
    "@sveltejs/adapter-auto": "^3.1.0",
    "@sveltejs/adapter-cloudflare": "^3.0.2",
    "@sveltejs/kit": "^2.3.3",
    "@sveltejs/vite-plugin-svelte": "^3.0.1",
    "@tailwindcss/nesting": "0.0.0-insiders.565cd3e",
    "@tailwindcss/typography": "^0.5.10",
    "@types/eslint": "8.56.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "autoprefixer": "^10.4.16",
    "clsx": "^2.1.0",
    "eslint-plugin-svelte": "^2.35.1",
    "lucide-svelte": "^0.312.0",
    "mode-watcher": "^0.1.2",
    "postcss": "^8.4.33",
    "postcss-import": "^16.0.0",
    "prettier-plugin-svelte": "^3.1.2",
    "svelte": "^4.2.8",
    "svelte-check": "^3.6.3",
    "tailwind-merge": "^2.2.0",
    "tailwind-variants": "^0.1.20",
    "tailwindcss": "^3.4.1",
    "tslib": "^2.6.2",
    "vite": "^5.0.11"
  },
  "type": "module",
  "dependencies": {
    "@auth/sveltekit": "^0.6.0",
    "@fontsource-variable/fira-code": "^5.0.16",
    "svelte-weather": "^0.5.2"
  }
}
