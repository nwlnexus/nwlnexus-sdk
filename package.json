{
  "name": "olympus-v4",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "postinstall": "remix-esbuild-override",
    "clean": "rimraf ./public/build \"./app/styles/**/*.css\" \"./functions/_assets/*\"",
    "build": "run-s \"build:*\"",
    "build:css": "postcss styles --base ./styles --dir ./app/styles --env production",
    "build:remix": "remix build",
    "dev": "concurrently \"npm:dev:*\"",
    "dev:css": "postcss styles --base ./styles --dir ./app/styles -w",
    "dev:remix": "remix watch",
    "dev:wrangler": "cross-env NODE_ENV=development wrangler pages dev ./public",
    "start": "cross-env NODE_ENV=production npm run dev:wrangler",
    "lint": "eslint --ext .tsx,.ts --ignore-path node_modules app",
    "lint:fix": "npm run lint -- --fix",
    "typecheck": "tsc -b",
    "format": "prettier --write \"./app/**/*.{ts,tsx}\""
  },
  "dependencies": {
    "@headlessui/react": "^1.6.6",
    "@remix-run/cloudflare": "^1.7.0",
    "@remix-run/cloudflare-pages": "^1.7.0",
    "@remix-run/react": "^1.7.0",
    "clsx": "^1.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.2",
    "remix-auth": "^3.2.2",
    "remix-auth-auth0": "^1.3.8",
    "tiny-invariant": "^1.2.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^3.14.1",
    "@esbuild-plugins/node-globals-polyfill": "^0.1.1",
    "@iconify/react": "^3.2.2",
    "@remix-run/dev": "^1.7.0",
    "@remix-run/eslint-config": "^1.7.0",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.7",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^29.0.0",
    "@types/react": "^18.0.18",
    "@types/react-dom": "^18.0.6",
    "autoprefixer": "^10.4.8",
    "concurrently": "^7.3.0",
    "cross-env": "^7.0.3",
    "cssnano": "^5.1.13",
    "esbuild-jest": "^0.5.0",
    "eslint": "8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest-dom": "^4.0.2",
    "eslint-plugin-testing-library": "^5.6.0",
    "jest": "^29.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.16",
    "postcss-cli": "^10.0.0",
    "postcss-import": "^15.0.0",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "remix-esbuild-override": "^3.0.4",
    "rimraf": "^3.0.2",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.7.4",
    "wrangler": "^2.0.28"
  },
  "engines": {
    "node": ">=16.13"
  }
}
