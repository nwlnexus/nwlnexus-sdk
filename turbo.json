{
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["build/**", "public/build/**", ".cache/**"]
    },
    "clean": {
      "cache": false
    },
    "dev": {
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "cache": false
    },
    "typecheck": {
      "outputs": ["tsconfig.tsbuildinfo"]
    }
  }
}
